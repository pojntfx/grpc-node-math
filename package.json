{
  "name": "math-grpc-node",
  "version": "1.0.0",
  "description": "A simple math GRPC microservice, for the purpose of learning and evaluating Node and gRPC.",
  "main": "src/lib/svc/math.js",
  "bin": "src/cmd/server/math.js",
  "scripts": {
    "dev": "node src/cmd/server/math.js",
    "build-protoc": "mkdir -p .generated && grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./.generated --grpc_out=./.generated --plugin=protoc-gen-grpc=./node_modules/.bin/grpc_tools_node_protoc_plugin ./src/lib/proto/math.proto",
    "build-pkg-amd64": "mkdir -p .bin && pkg ./src/cmd/server/math.js -t linux-x64 -o .bin/math-grpc-node-server-amd64",
    "build-pkg-arm64": "mkdir -p ~/.pkg-cache/v2.6/ && wget https://github.com/robertsLando/pkg-binaries/blob/master/arm64/fetched-v12.2.0-linux-arm64?raw=true -O ~/.pkg-cache/v2.6/fetched-v12.2.0-linux-arm64 && mkdir -p .bin && pkg ./src/cmd/server/math.js -t linux-arm64 -o .bin/math-grpc-node-server-arm64",
    "build": "npm run build-protoc && npm run build-pkg-amd64; npm run build-pkg-arm64 &> /dev/null; chmod +x ./.bin/*",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pojntfx/grpc-node-math.git"
  },
  "author": "Felix Pojtinger <felix@pojtinger.com>",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/pojntfx/grpc-node-math/issues"
  },
  "homepage": "https://github.com/pojntfx/grpc-node-math#readme",
  "devDependencies": {
    "grpc-tools": "^1.8.0",
    "pkg": "^4.4.0"
  },
  "dependencies": {
    "commander": "^4.0.0",
    "google-protobuf": "^3.10.0",
    "grpc": "^1.24.2"
  }
}
