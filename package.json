{
	"name": "mather.js",
	"version": "0.0.2",
	"description": "A simple math GRPC microservice, for the purpose of learning and evaluating Node and gRPC.",
	"main": "./.build/index.js/index.js",
	"bin": {
		"math-grpc-node-server": "./.build/index.js/index.js"
	},
	"scripts": {
		"protoc-build": "gulp",
		"ncc-build": "gulp nccBuild",
		"pkg-build-binary-linux-amd64": "gulp pkgBuildBinary --platform linux --architecture amd64",
		"pkg-build-binary-darwin-amd64": "gulp pkgBuildBinary --platform darwin --architecture amd64",
		"pkg-install-binary-linux-amd64": "gulp pkgInstallBinary --platform linux --architecture amd64",
		"pkg-install-binary-darwin-amd64": "gulp pkgInstallBinary --platform darwin --architecture amd64",
		"build": "npm run protoc-build && npm run ncc-build",
		"clean": "gulp clean",
		"run": "gulp run",
		"start": "npm run run",
		"watch": "gulp run",
		"dev": "npm run watch",
		"prepare": "npm run build",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pojntfx/grpc-node-math.git"
	},
	"author": "Felix Pojtinger <felix@pojtinger.com>",
	"license": "AGPL-3.0",
	"bugs": {
		"url": "https://github.com/pojntfx/grpc-node-math/issues"
	},
	"homepage": "https://github.com/pojntfx/grpc-node-math#readme",
	"devDependencies": {
		"@zeit/ncc": "^0.20.5",
		"grpc-tools": "^1.8.0",
		"gulp": "^4.0.2",
		"pkg": "^4.4.0",
		"shelljs": "^0.8.3"
	},
	"dependencies": {
		"commander": "^4.0.0",
		"google-protobuf": "^3.10.0",
		"grpc": "^1.24.2"
	},
	"publishConfig": {
		"access": "public"
	}
}
